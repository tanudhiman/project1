
<%= bootstrap_devise_error_messages! %>

  <%= video_tag "Productive-Morning.mp4", controls: false, autoplay: true, muted:
       true, loop: true, class: "video-container", type:"video/mp4" %>

<div class="overlay1">
  <div >
 <center>  <h2 class="head"> smartData Enterprises</h2></center>
    <center>  <h4 class="head2">ONLINE PROJECT MANAGEMENT SYSTEM</h4></center></div>

 <div>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { role: 'form' }) do |f| %>

      <div >
        <%= f.label :email %>
        <%= f.email_field :email, autofocus: true,autocomplete:"off", class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :password %>
        <%= f.password_field :password, autocomplete: 'off', class: 'form-control' %>
      </div>
      <% if devise_mapping.rememberable? %>
        <div class="checkbox">
          <%= f.label :remember_me do %>
            <%= f.check_box :remember_me %> <%= t('.remember_me', default: 'Remember me') %>
          <% end %>
        </div>
      <% end %>
      <%= f.submit  t('.sign_in', default: 'Sign in'), class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
<%=render 'layouts/icons'%>

<p class="a" > <%= render 'devise/shared/links' %></p>


</div>
<%= render 'layouts/footor'%>
